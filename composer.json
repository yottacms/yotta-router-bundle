{
    "name": "yottacms/yotta-router-bundle",
    "description": "Yotta router bundle",
    "keywords": [
        "yottacms",
        "symfony",
        "dependencies",
        "bundle"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Hrischanovich",
            "email": "info@asdf.by"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony-cmf/routing-bundle": "dev-master"
    },
    "require-dev": {
		"symfony/dependency-injection": "^3.4||^4.0",
		"symfony/config": "^3.4||^4.0",
        "phpunit/phpunit": "^5.6.4"
    },
    "autoload": {
        "psr-4": {
            "YottaCms\\Bundle\\YottaRouterBundle\\": ""
        },
		"exclude-from-classmap": [
            "Tests/"
        ]
    }
}
